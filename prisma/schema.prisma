generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pokemon {
  id     Int    @id @default(autoincrement())
  name   String
  userid Int
  User   User   @relation(fields: [userid], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model User {
  id       Int       @id @default(autoincrement())
  email    String    @unique
  password String
  Pokemon  Pokemon[]
}
